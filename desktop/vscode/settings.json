// Place your settings in this file to overwrite the default settings
{
  "editor.tabSize": 2,
  "window.menuBarVisibility": "toggle",
  "editor.wordWrap": "on",
  "files.associations": {
      "Jenkinsfile": "groovy",
      "Vagrantfile": "ruby"
  },
  "update.channel": "none",
  "window.zoomLevel": 0,
  "keyboard.dispatch": "keyCode",
  "typescript.check.tscVersion": false,

  "workbench.iconTheme": "material-icon-theme",

  "python.linting.enabled": false,

  "rust.rustLangSrcPath": "/usr/src/rust/src",
  "vsicons.dontShowNewVersionMessage": true,
  "python.linting.enabledWithoutWorkspace": false,

  "latex-workshop.latex.toolchain": [
    {
      "command": "xelatex",
      "args": [
        "--synctex",
        "--pdf",
        "--tex-option=\"-interaction=nonstopmode\"",
        "--tex-option=\"-file-line-error\"",
        "%DOC%.tex"
      ]
    }
  ],

  "material-icon-theme.showUpdateMessage": false,
  "extensions.ignoreRecommendations": false,
  "explorer.confirmDragAndDrop": false,
  "vsicons.projectDetection.autoReload": true,
  "terminal.integrated.rendererType": "dom",
  "update.enableWindowsBackgroundUpdates": false,
  "window.restoreWindows": "none",
  "liveshare.featureSet": "insiders",
  "files.eol": "\n",
  "cwtools.cache.stellaris": "/home/lindenk/.steam/steam/steamapps/common/Stellaris",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.jediEnabled": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.colorTheme": "Complete Dark",
  "rust.mode": "rls",
  "rust.rustup": {
    "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu"
  },
  "rust.rls": {
    "useRustfmt": true
  }
}
